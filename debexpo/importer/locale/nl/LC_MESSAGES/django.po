msgid "{package}_{version}: ACCEPTED on {site} ({distribution})"
msgstr "{package}_{version}: GEACCEPTEERD op {site} ({distribution})"

#: importer/models.py
msgid "Changes is invalid"
msgstr "Changes is ongeldig"

#: importer/models.py
msgid "Dsc failed to parse"
msgstr "Dsc kon niet ontleed worden"

#: importer/models.py
