#: debexpo/tools/debian/changes.py:79
#, python-brace-format
msgid "Changes file {filename} could not be parsed"
msgstr "Il file delle modifiche {filename} non può essere analizzato"

#: debexpo/tools/debian/changes.py:134
#, python-brace-format
msgid "Changes file invalid. Missing key {key}"
msgstr "Il file delle modifiche è invalido. Chiave {key} mancante"

#: debexpo/tools/debian/changes.py:185
msgid ""
"dsc is missing from changes\n"
"Make sure you include the full source (if you are using sbuild make sure to "
"use the --source option or the equivalent configuration item; if you are "
"using dpkg-buildpackage directly use the default flags or -S for a source "
"only upload)"
msgstr ""
"dsc è mancante dalle modifiche\n"
"Assicurarsi di includere il sorgente completo (se stai usando sbuild "
"assicurati di usare l'opzione --source o l'elemento di configurazione "
"equivalente; se stai usando dpkg-buildpackage, allora usare direttamente i "
"flag predefiniti o -S per un caricamento del solo sorgente)"

#: debexpo/tools/debian/changes.py:210
#, python-brace-format
msgid ""
"{source} exists in the official Debian archive with the version {version}/"
"{distribution}. You may not upload a lower or equal version to this one."
msgstr ""
"{source} esiste nell'archivio ufficiale di Debian con la versione "
"{version}/{distribution}. Non devi caricare una versione minore o uguale a "
"questa."

#: debexpo/tools/debian/control.py:41
#, python-brace-format
msgid "Failed to parse debian/control:{message}"
msgstr "Analisi di debian/control fallita:{message}"

#: debexpo/tools/debian/control.py:175
msgid "No source definition found"
msgstr "Definizione dei sorgenti non trovata"

#: debexpo/tools/debian/control.py:178
msgid "No binary definition found"
msgstr "Definizione degli eseguibili non trovata"

#: debexpo/tools/debian/control.py:185 debexpo/tools/debian/control.py:191
#, python-brace-format
msgid "Missing key {key} in source definition"
msgstr "Chiave {key} mancante nella definizione dei sorgenti"

#: debexpo/tools/debian/copyright.py:38
#, python-brace-format
msgid "Failed to parse debian/copyright: {message}"
msgstr "Analisi di debian/copyright fallita: {message}"

#: debexpo/tools/debian/dsc.py:53
#, python-brace-format
msgid "{dsc} could not be parsed"
msgstr "{dsc} non può essere analizzato"

#: debexpo/tools/debian/dsc.py:86
#, python-brace-format
msgid "Missing key {key}"
msgstr "Chiave {key} mancante"

#: debexpo/tools/debian/origin.py:91
#, python-brace-format
msgid "Failed to retrive origin info: {e}"
msgstr "Recupero delle informazioni dell'origine fallito: {e}"

#: debexpo/tools/debian/origin.py:104
#, python-brace-format
msgid ""
"Source package origin file differs from the official archive:\n"
"\n"
"Origin file         : {origin}\n"
"\n"
"sha256sum in upload : {upload}\n"
"sha256sum in archive: {archive}"
msgstr ""
"Il file originale del pacchetto dei sorgenti è diverso da quello "
"dell'archivio ufficiale:\n"
"\n"
"File originale         : {origin}\n"
"\n"
"sha256sum in caricamento : {upload}\n"
"sha256sum in archivio: {archive}"

#: debexpo/tools/debian/source.py:65
#, python-brace-format
msgid "Internal error. Please contact debexpo administrators at {email}"
msgstr ""
"Errore interno. Contattare gli amministratori di debexpo all'indirizzo "
"{email}"

#: debexpo/tools/debian/source.py:68
#, python-brace-format
msgid "Could not extract source package from {dsc}: {e}"
msgstr "Non posso estrarre il pacchetto dei sorgenti da {dsc}: {e}"

#: debexpo/tools/debian/source.py:72
#, python-brace-format
msgid "Could not extract source package from {dsc}: extraction took too long"
msgstr ""
"Non posso estrarre il pacchetto dei sorgenti da {dsc}: l'estrazione ha "
"richiesto troppo tempo"

#: debexpo/tools/files.py:56
#, python-brace-format
msgid "No checksum method available for file {filename}."
msgstr "Nessun metodo di checksum disponibile per il file {filename}."

#: debexpo/tools/files.py:67
#, python-brace-format
msgid ""
"Checksum failed for file {filename}.\n"
"\n"
"Expected: {expected}\n"
"Computed: {computed}"
msgstr ""
"Checksum fallito per il file {filename}.\n"
"\n"
"Atteso: {expected}\n"
"Calcolato: {computed}"

#: debexpo/tools/files.py:143
#, python-brace-format
msgid "{filename} is missing from upload"
msgstr "{filename} è mancante dal caricamento"

#: debexpo/tools/gnupg.py:73 debexpo/tools/gnupg.py:77
#, python-brace-format
msgid ""
"Unable to verify file {filename}. No public key found for key {fingerprint}"
msgstr ""
"Impossibile verificare il file {filename}. Nessuna chiave pubblica trovata "
"per la chiave {fingerprint}"

#: debexpo/tools/gnupg.py:155
#, python-brace-format
msgid "{filename}: not a GPG signed file"
msgstr "{filename}: non è un file firmato con GPG"

#: debexpo/tools/gnupg.py:163
#, python-brace-format
msgid "Unknown GPG error. Output was: {output}"
msgstr "Errore GPG sconosciuto. È stato restituito: {output}"

#: debexpo/tools/gnupg.py:183
#, python-brace-format
msgid "Cannot add key: {key}"
msgstr "Non posso aggiungere la chiave: {key}"
